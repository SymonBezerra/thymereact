{
    "presets": [
      ["@babel/preset-env", {
        "targets": "> 0.25%, not dead"
      }],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime",
      ["@babel/plugin-transform-modules-umd", {
        "globals": {
          "react": "React",
          "react-dom": "ReactDOM",
          "react-dom/client": "ReactDOM"
        },
        "exactGlobals": true
      }]
    ]
  }